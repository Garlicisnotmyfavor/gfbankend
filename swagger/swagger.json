    "basePath": "/v1",
    "paths": {
        "/api/user/": {
            "get": {
                "tags": [
                    "api/user"
                ],
                "description": "show all cards",
                "operationId": "UserController.showAllCards",
                "parameters": [
                    {
                        "in": "body",
                        "name": "body",
                        "description": "true",
                        "schema": {
                            "$ref": "#/definitions/models.Card"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Read successfully"
                    },
                    "404": {
                        "description": "Fail to read"
                    }
                }
            }
        },
        "/api/user/card/{id}/add": {
            "get": {
                "tags": [
                    "api/user"
                ],
                "description": "将这个user的id和卡绑定",
                "operationId": "CardController.AddCard",
                "parameters": [
                    {
                        "in": "query",
                        "name": "id",
                        "description": "原本的卡号",
                        "required": true,
                        "type": "string"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "返回绑定的卡的大致信息",
                        "schema": {
                            "$ref": "#/definitions/models.Card"
                        }
                    },
                    "403": {
                        "description": "绑定的卡片不存在"
                    }
                }
            }
        },
        "/api/user/card/{id}/coupons": {
            "post": {
                "tags": [
                    "api/user"
                ],
                "description": "增加或减少某张卡的某种优惠券",
                "operationId": "CardController.coupons",
                "responses": {
                    "200": {
                        "description": ""
                    },
                    "400/404/406": {
                        "description": "json解析错误/卡不存在/非法数据"
                    }
                }
            }
        },
        "/api/user/card/{id}/info": {
            "put": {
                "tags": [
                    "api/user"
                ],
                "description": "修改卡片的卡号，公司",
                "operationId": "CardController.ModifyCardInfo",
                "parameters": [
                    {
                        "in": "query",
                        "name": "id",
                        "description": "原本的卡号",
                        "required": true,
                        "type": "string"
                    },
                    {
                        "in": "body",
                        "name": "cardInfo",
                        "description": "新卡信息(卡号+公司)",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/json"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "修改成功，返回新卡片对象",
                        "schema": {
                            "$ref": "#/definitions/models.Card"
                        }
                    },
                    "400": {
                        "description": "body解析错误"
                    },
                    "404": {
                        "description": "卡片信息读取错误"
                    },
                    "500": {
                        "description": "数据库更新操作错误"
                    }
                }
            }
        }
    },
    "definitions": {
        "json": {
            "title": "json",
            "type": "object"
        },
        "models.Card": {
            "title": "Card",
            "type": "object",
            "properties": {
                "BatchNum": {
                    "type": "integer",
                    "format": "int64"
                },
                "CardId": {
                    "type": "string"
                },
                "CardOrder": {
                    "type": "integer",
                    "format": "int64"
                },
                "CardType": {
                    "type": "string"
                },
                "City": {
                    "type": "string"
                },
                "CouponsList": {
                    "type": "string"
                },
                "CouponsNum": {
                    "type": "string"
                },
                "DelTime": {
                    "type": "string",
                    "format": "datetime"
                },
                "Enterprise": {
                    "type": "string"
                },
                "ExpireTime": {
                    "type": "string",
                    "format": "datetime"
                },
                "FactoryNum": {
                    "type": "integer",
                    "format": "int64"
                },
                "Money": {
                    "type": "integer",
                    "format": "int64"
                },
                "ScoreList": {
                    "type": "string"
                },
                "ScoreNum": {
                    "type": "string"
                },
                "SerialNum": {
                    "type": "integer",
                    "format": "int64"
                },
                "State": {
                    "type": "string"
                },
                "UserId": {
                    "type": "string"
                }
            }
        }
    }
}